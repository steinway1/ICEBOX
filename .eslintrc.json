{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-console": [
      "error",
      {
        "allow": ["warn", "error", "log"]
      }
    ],
    "semi": ["error", "never"]
  }
}