<meta charset="UTF-8">
<title>
	{% if page_title is defined %}
		{{page_title}}
	{% else %}
		{{page_default_title}}
	{% endif %}
</title>

<meta name="description" content="{% if page_description is defined %}{{page_description}}{% else %}{{page_default_description}}{% endif %}">
<meta name="keywords" content="{% if page_default_keywords %}{{page_default_keywords}}{% else %}{% endif %}">
<meta name="robots" content="index follow">

<link rel="stylesheet" href="css/partial_v2.css"/>

<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

<meta name="viewport" content="width=device-width, height=device-height, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no"/>
<meta name="theme-color" content="#ffffff" media="(prefers-color-scheme: light)">


