<div class="am-content w_max_1080">
	<div
		class="am-content__wrapper">

		{# MFC - Manual Orders #}
		<div class="am-main__header">
			<div class="am-container">
				<div class="am-row__header-wrapper">
					<div class="am-row__header">
						<div class="am_vertical-start">
							<h2 class="am-main__header-title">Manufacturing. Manual Orders.</h2>
							<span class="am-main__subtitle">Manual Orders</span>
						</div>
					</div>
					<div class="am_flex8">
						<div class="am-select-wrap">
							<select class="am-select" id="mfgSelect">
								<option value="0">Unselected</option>
								<option value="evique">Evique</option>
								<option value="recieved">Received Orders</option>
								<option value="veda">Veda</option>
								<option value="gs">GS</option>
								<option value="premier">Premier</option>
								<option value="shipped">shipped</option>
								<option value="in-house">In-house</option>
								<option value="americas-gold">Americas Gold</option>
								<option value="alessi">Alessi</option>
								<option value="fancy-chains">Fancy Chains</option>
								<option value="melano">Melano</option>
								<option value="daniel-jewelry">Daniel Jewelry</option>
								<option value="haimov">Haimov</option>
								<option value="deco">Deco</option>
							</select>
						</div>
						<button class="am-mfc-btn" onclick="exportList();">Export</button>
						<a href="/assets/public-2020/files/instore-orders-template.xlsx" class="am-mfc-btn">Download Template</a>
					</div>
					<div class="am-upload-input">
						{% include "admin/svg/icon_input.svg" %}
						<div>
							<span>Click To Upload Excel File</span>
							<span>Upload In-Store Orders</span>
						</div>
					</div>
				</div>
			</div>
		</div>

		<!-- #region Content : Main -->
		<div class="am-main__flow">
			<div class="am-container">
				<div class="am-main__content">
					<div class="am__dbl-wrap">
						<div class="am-main__orders">
							<div class="am-content-divider">{{orders|length}}
								Orders Total<span></span>
							</div>
              {% include "admin/components/pagination.twig" %}
							{# ORDERS #}
							{% for order in orders %}
								{% include "admin/components/item-mfc-manual.twig" %}
							{% endfor %}
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- #endregion -->


	</div>
</div>
